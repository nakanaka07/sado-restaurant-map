# 開発環境設定例
# このファイルをコピーして .env.development を作成してください

# === 必須環境変数 ===
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_BASE_URL=/

# === アプリケーション設定 ===
VITE_APP_NAME=佐渡飲食店マップ（開発）
VITE_APP_VERSION=dev

# === 開発環境用設定 ===
VITE_ENABLE_PWA_DEV=true
VITE_DEBUG_MODE=true
VITE_MOCK_DATA=true
VITE_LOG_LEVEL=debug
VITE_HOT_RELOAD=true

# === 開発サーバー設定 ===
VITE_DEV_PORT=5173
VITE_DEV_HOST=localhost

# === デバッグ設定 ===
VITE_REACT_DEVTOOLS=true
VITE_REDUX_DEVTOOLS=true
VITE_CONSOLE_LOGS=true

# === 外部サービス連携（開発用） ===
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_SHEETS_API_KEY=your_dev_sheets_api_key_here
VITE_SPREADSHEET_ID=your_dev_spreadsheet_id_here

# === セキュリティ設定（開発用） ===
VITE_ALLOWED_ORIGINS=https://localhost:5173,http://localhost:5173
