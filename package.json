{
  "name": "sado-restaurant-map",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --config config/eslint.config.js",
    "preview": "vite preview",
    "test": "vitest --config config/vitest.config.ts",
    "test:run": "vitest run --config config/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config config/vitest.config.ts",
    "deploy": "pnpm run build && gh-pages -d dist",
    "predeploy": "pnpm run build",
    "generate:pwa-assets": "pwa-assets-generator --config config/pwa-assets.config.ts",
    "data:update": "powershell -ExecutionPolicy Bypass -File scripts/database-operations.ps1",
    "test:integration": "powershell -ExecutionPolicy Bypass -File scripts/test-integration.ps1"
  },
  "dependencies": {
    "@types/google.maps": "^3.58.1",
    "@vis.gl/react-google-maps": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.12",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.11",
    "vite-plugin-pwa": "^0.21.1",
    "vitest": "^3.2.4",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
