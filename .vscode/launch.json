{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "msedge",
      "name": "üçΩÔ∏è Sado Restaurant Map (Edge) - Vite 7.1.4",
      "request": "launch",
      "url": "http://127.0.0.1:5173/",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "userDataDir": "${workspaceFolder}/.vscode/edge-user-data",
      "runtimeArgs": [
        "--remote-debugging-port=9225",
        "--disable-features=VizDisplayCompositor",
        "--disable-background-timer-throttling",
        "--disable-renderer-backgrounding",
        "--disable-backgrounding-occluded-windows"
      ],
      "env": {
        "VITE_DEBUG": "true"
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "msedge",
      "name": "üçΩÔ∏è Sado Restaurant Map (Edge - Safe Mode)",
      "request": "launch",
      "url": "http://127.0.0.1:5173/",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "userDataDir": "${workspaceFolder}/.vscode/edge-user-data-safe",
      "runtimeArgs": [
        "--remote-debugging-port=9226"
      ],
      "env": {
        "VITE_DEBUG": "true"
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "chrome",
      "name": "üçΩÔ∏è Sado Restaurant Map (Chrome) - Vite 7.1.4",
      "request": "launch",
      "url": "http://127.0.0.1:5173/",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "userDataDir": "${workspaceFolder}/.vscode/chrome-user-data",
      "runtimeArgs": [
        "--disable-web-security",
        "--disable-dev-shm-usage",
        "--no-sandbox",
        "--remote-debugging-port=9227",
        "--disable-extensions"
      ],
      "env": {
        "VITE_DEBUG": "true"
      },
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "firefox",
      "name": "üçΩÔ∏è Sado Restaurant Map (Firefox) - Vite 7.1.4",
      "request": "launch",
      "url": "http://127.0.0.1:5173/",
      "webRoot": "${workspaceFolder}/src",
      "profile": "default",
      "preferences": {
        "devtools.debugger.remote-enabled": true,
        "devtools.chrome.enabled": true,
        "devtools.debugger.workers": true
      }
    },
    {
      "type": "node",
      "name": "üîß Vite Development Server Debug",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/vite",
      "args": [
        "--host",
        "127.0.0.1",
        "--port",
        "5173"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "vite:*"
      },
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "msedge",
      "name": "üçΩÔ∏è Preview (Prod Build)",
      "request": "launch",
      "url": "http://127.0.0.1:4173/sado-restaurant-map/",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true
    }
  ]
}
